int singleNumber(int* nums, int numsSize){
    
    int i , j, sum = 0, result= 0;
    
    for(i = 0; i<32; i++){
        sum = 0;
        for(j = 0; j<numsSize; j++){
             sum += (nums[j]>>i & 1) ;
        }
        if(sum%3 == 1)
            result = result | (long long)1<< i;
    }
    return result;
}
