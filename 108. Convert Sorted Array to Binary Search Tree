struct TreeNode *helperp(int *a,int start, int end){
    
    struct TreeNode *root;
    if(start > end)
        return NULL;
  
    int mid = (start + end)/2;
    root = (struct TreeNode *)malloc(sizeof(struct TreeNode));
    root->left = root->right = NULL;
    root->val = a[mid];
    
    root->left = helperp(a, start, mid-1);
    root->right = helperp(a,  mid+1, end);
    
   return root;    
    
}
struct TreeNode* sortedArrayToBST(int* nums, int numsSize){

    if(nums == NULL || numsSize <=0)
        return 0;
    
    return helperp(nums,0,numsSize-1);
    
}
